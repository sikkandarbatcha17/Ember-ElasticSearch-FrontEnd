<body>
<main class="m-3">
	<div class="row col-md-6">
		<table class="table table-striped table-bordered table-sm">
	            <tr>
	                <th>Top Values</th>
	                <th>Count</th>
	            </tr>
                {{#each this.topHits as |topHit|}}
                    <tr>
                    <td>Key: {{topHit.topValue}}</td>
                    <td>Value: {{topHit.count}}</td>
                </tr>
                {{/each}}
            </table>
     </div>
     <<LinkTo @route="protectedPage1">Go Back</LinkTo>
    <br><br>
     <<LinkTo @route="protectedPage">Go to Home Page</LinkTo>
     <br><br>
     <button type="submit" {{on 'click' this.invalidate }} >Logout</button>
</main>
</body>
